# Minecraft Bot Configuration
MC_HOST=your_server_address
MC_PORT=25565
MC_USERNAME=your_minecraft_email_or_username
MC_PASSWORD=your_minecraft_password_or_leave_blank_for_offline_mode
# For Microsoft accounts, set MC_AUTH to 'microsoft'. For Mojang, you can omit or set to 'mojang'.
# For offline mode servers, you might not need MC_PASSWORD and can omit MC_AUTH.
MC_AUTH=microsoft # or mojang or omit for offline/unauthenticated
MC_VERSION=false # Example: "1.20.1", or false to auto-detect

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key

# Server Configuration (for Render)
PORT=3000
